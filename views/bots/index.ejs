<!DOCTYPE html>
<html lang="en">

<head>
  

    
    <link rel="stylesheet" href="/bootstrap.min.css">
    <link rel="stylesheet" href="/style.css">
    <link href="https://fonts.googleapis.com/css2?family=Play&display=swap" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
        integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
        integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"
        integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI"
        crossorigin="anonymous"></script>
    <title>Discord AR</title>
    <style>
        strong { color: #FFD700}
        .bg-gold { background: #FFD700}
    </style>
</head>

<body onload="emojify()">
    <div class="container">
        <%- include("../part/nav.ejs") %>
        <div class="jumbotron bg-dark2 text-white mt-5 rounded">
            <h1 class="text-center">Discord AR List</h1>
            <br>
            <form method="GET" action="/bots/search" class="form-inline">
                <center>
                    <input style="width: 100%;" type="text" class="form-control" id="search" name="q" required>
                </center>
                <button type="submit" class="ml-3 btn btn-outline-info rounded">Search</button>
            </form>
            <br>
            <br>
            <br>
            <h1 style="color: gold;"># Featured Bots</h1>
            <br>
            <div class="row">
                <% featured.forEach(tb => { %>
                <div class="col-sm-3 mb-4">
                    <div class="card bg-dark rounded text-white">
                        <% if(bot.booster.has(tb.owner.id)) { %>
                            <img src="<%= tb.bannerURL || `/Logo.png` %>" alt="background" class="card-bg" draggable="false">
                        <%} else {%>
                            <img src="/Logo.png" alt="background" class="card-bg" draggable="false">
                        <% } %>
                        <img src="<%= tb.avatarURL %>" alt="image" class="card-imge" draggable="false">
                        <span class="bot-upvotes badge rounded badge-info"
                            style="font-size: 14px;width: 40%;margin-left: 140px;margin-top: -55px;">Upvotes:
                            <%= tb.upvotes %></span>
                        <div class="card-body mt-4">
                            <h5 class="card-title"><%= tb.username %></h5>
                            <p class="card-text"><%= tb.short_description %></p>
                            <a href="/bots/<%= tb.id %>" class="btn btn-success rounded">View</a>
                            <a href="<%= tb.invite %>" target="_blank" class="btn btn-info rounded">Invite</a>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
            <br>
            <hr>
            <br>
            <h1># Top Bots</h1>
            <br>
            <div class="row">
                <% top.forEach(tb => { %>
                <div class="col-sm-3 mb-4">
                    <div class="card bg-dark rounded text-white">
                        <% if(bot.booster.has(tb.owner.id)) { %>
                            <img src="<%= tb.bannerURL || `/Logo.png` %>" alt="background" class="card-bg" draggable="false">
                        <%} else {%>
                            <img src="/Logo.png" alt="background" class="card-bg" draggable="false">
                        <% } %>                        <img src="<%= tb.avatarURL %>" alt="image" class="card-imge" draggable="false">
                        <span class="bot-upvotes badge rounded badge-info"
                            style="font-size: 14px;width: 40%;margin-left: 140px;margin-top: -55px;">Upvotes:
                            <%= tb.upvotes %></span>
                        <div class="card-body mt-4">
                            <h5 class="card-title"><%= tb.username %></h5>
                            <p class="card-text"><%= tb.short_description %></p>
                            <a href="/bots/<%= tb.id %>" class="btn btn-success rounded">View</a>
                            <a href="<%= tb.invite %>" target="_blank" class="btn btn-info rounded">Invite</a>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
            <br>
            <hr>
            <br>
            <h1># Random Bots</h1>
            <br>
            <div class="row">
                <% random.forEach(tb => { %>
                <div class="col-sm-3 mb-4">
                    <div class="card bg-dark rounded text-white">
                        <% if(bot.booster.has(tb.owner.id)) { %>
                            <img src="<%= tb.bannerURL || `/Logo.png` %>" alt="background" class="card-bg" draggable="false">
                        <%} else {%>
                            <img src="/Logo.png" alt="background" class="card-bg" draggable="false">
                        <% } %>                        <img src="<%= tb.avatarURL %>" alt="image" class="card-imge" draggable="false">
                        <span class="bot-upvotes badge rounded badge-info"
                            style="font-size: 14px;width: 40%;margin-left: 140px;margin-top: -55px;">Upvotes:
                            <%= tb.upvotes %></span>
                        <div class="card-body mt-4">
                            <h5 class="card-title"><%= tb.username %></h5>
                            <p class="card-text"><%= tb.short_description %></p>
                            <a href="/bots/<%= tb.id %>" class="btn btn-success rounded">View</a>
                            <a href="<%= tb.invite %>" target="_blank" class="btn btn-info rounded">Invite</a>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
            <br>
            <hr>
            <br>
            <h1># New Bots</h1>
            <br>
            <div class="row">
                <% newBots.forEach(tb => { %>
                <div class="col-sm-3 mb-4">
                    <div class="card bg-dark rounded text-white">
                        <% if(bot.booster.has(tb.owner.id)) { %>
                            <img src="<%= tb.bannerURL || `/Logo.png` %>" alt="background" class="card-bg" draggable="false">
                        <%} else {%>
                            <img src="/Logo.png" alt="background" class="card-bg" draggable="false">
                        <% } %>                        <img src="<%= tb.avatarURL %>" alt="image" class="card-imge" draggable="false">
                        <span class="bot-upvotes badge rounded badge-info"
                            style="font-size: 14px;width: 40%;margin-left: 140px;margin-top: -55px;">Upvotes:
                            <%= tb.upvotes %></span>
                        <div class="card-body mt-4">
                            <h5 class="card-title"><%= tb.username %></h5>
                            <p class="card-text"><%= tb.short_description %></p>
                            <a href="/bots/<%= tb.id %>" class="btn btn-success rounded">View</a>
                            <a href="<%= tb.invite %>" target="_blank" class="btn btn-info rounded">Invite</a>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
        <%- include("../part/footer.ejs") %>
    </div>
</body>

</html>